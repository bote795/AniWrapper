{
  "name": "aniwrapper",
  "version": "1.0.0",
  "description": "api wrapper for anilist",
  "module": "dist/Anilist.js",
  "scripts": {
    "postinstall": "cp ./githooks/pre-push .git/hooks/ && chmod +x .git/hooks/pre-push",
    "dev": "webpack --watch",
    "build:node": "cp dist/Anilist.js example/ && node example/index.js",
    "deploy": "webpack -p && npm run build:node && npm run cover:only",
    "test:only": "mocha --require babel-core/register ",
    "test": "npm run test:only",
    "lint": "eslint lib test",
    "docs": "./node_modules/.bin/jsdoc lib/Anilist.js -d docs",
    "cover:only": "istanbul cover _mocha -- --require babel-core/register",
    "cover": "istanbul cover _mocha --report lcovonly -- --require babel-core/register -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bote795/AniWrapper.git"
  },
  "keywords": [
    "anilist",
    "api",
    "graphql",
    "v2"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bote795/AniWrapper/issues"
  },
  "homepage": "https://github.com/bote795/AniWrapper#readme",
  "dependencies": {
    "graphql-request": "^1.6.0",
    "jwt-decode": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "istanbul": "^1.0.0-alpha",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  }
}
